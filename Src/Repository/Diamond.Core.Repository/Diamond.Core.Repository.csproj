<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net9.0</TargetFramework>
    <Product>Diamond.Core Patterns Library</Product>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
    <Authors>Daniel M Porrey</Authors>
    <PackageProjectUrl>https://github.com/porrey/Diamond.Core-Patterns</PackageProjectUrl>
    <RepositoryUrl>https://github.com/porrey/Diamond.Core-Patterns</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <Version>9.1.0</Version>
    <FileVersion>9.1.0</FileVersion>
    <AssemblyVersion>9.1.0</AssemblyVersion>
    <Copyright>Copyright Â© Daniel Porrey 2019-2025</Copyright>
    <Description>This library provides basic implementations of the repository pattern.</Description>
    <PackageIcon>Diamond.Core.128x128.png</PackageIcon>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageIconUrl>
    </PackageIconUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
  </PropertyGroup>
  <PropertyGroup>
    <DocumentationFile>Diamond.Core.Repository.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Diamond.Core.Repository.xml" />
    <None Remove="Services\Diamond.Core.Repository.json" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Services\Diamond.Core.Repository.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\..\Images\Diamond.Core.128x128.png">
      <Pack>True</Pack>
      <PackagePath>
      </PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="9.0.7" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="9.0.7" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Clonable\Diamond.Core.Clonable\Diamond.Core.Clonable.csproj" />
    <ProjectReference Include="..\Diamond.Core.Repository.Abstractions\Diamond.Core.Repository.Abstractions.csproj" />
    <ProjectReference Include="..\..\Shared\Diamond.Core.Abstractions\Diamond.Core.Abstractions.csproj" />
  </ItemGroup>
</Project>