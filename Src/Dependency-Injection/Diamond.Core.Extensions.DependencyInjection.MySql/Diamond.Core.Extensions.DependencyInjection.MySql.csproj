<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;net5.0</TargetFrameworks>
		<Product>Diamond.Core Patterns Library</Product>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<Version>5.0.0</Version>
		<Authors>Daniel M Porrey</Authors>
		<Description>This library provides JSON file based dependency injection for MySql DbContext.</Description>
		<Copyright>Copyright Â© Daniel Porrey 2019-2021</Copyright>
		<PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/porrey/Diamond.Core-Patterns</PackageProjectUrl>
		<RepositoryUrl>https://github.com/porrey/Diamond.Core-Patterns</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<FileVersion>5.0.0</FileVersion>
		<AssemblyVersion>5.0.0</AssemblyVersion>
		<PackageIcon>Diamond.Core.128x128.png</PackageIcon>
		<NeutralLanguage>en-US</NeutralLanguage>
		<PackageIconUrl></PackageIconUrl>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
	</PropertyGroup>

	<PropertyGroup>
	  <DocumentationFile>Diamond.Core.Extensions.DependencyInjection.MySql.xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Diamond.Core.Extensions.DependencyInjection.MySql.xml" />
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Pomelo.EntityFrameworkCore.MySql" Version="3.2.4" />
	  <PackageReference Include="Microsoft.Extensions.Hosting" Version="5.0.0" />
	</ItemGroup>

  <ItemGroup>
    <None Include="..\..\..\Images\Diamond.Core.128x128.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Diamond.Core.Extensions.DependencyInjection.Abstractions\Diamond.Core.Extensions.DependencyInjection.Abstractions.csproj" />
    <ProjectReference Include="..\Diamond.Core.Extensions.DependencyInjection.EntityFrameworkCore\Diamond.Core.Extensions.DependencyInjection.EntityFrameworkCore.csproj" />
  </ItemGroup>

</Project>
