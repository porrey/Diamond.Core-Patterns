<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageLicenseUrl>
    </PackageLicenseUrl>
    <Product>Diamond.Core Patterns Library</Product>
    <Copyright>Copyright Â© Daniel Porrey 2019-2024</Copyright>
    <Description>This library provides workflow interfaces and abstract or default implementations for common patterns.</Description>
    <Version>8.0.0.0</Version>
    <FileVersion>8.0.0.0</FileVersion>
    <AssemblyVersion>8.0.0.0</AssemblyVersion>
    <PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
    <Authors>Daniel M. Porrey</Authors>
    <PackageProjectUrl>https://github.com/porrey/Diamond.Core-Patterns</PackageProjectUrl>
    <RepositoryUrl>https://github.com/porrey/Diamond.Core-Patterns</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageIcon>Diamond.Core.128x128.png</PackageIcon>
    <NeutralLanguage>en-US</NeutralLanguage>
    <PackageIconUrl>https://raw.githubusercontent.com/porrey/Diamond.Core/master/Images/Diamond.Core.128x128.png</PackageIconUrl>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
  </PropertyGroup>
  <PropertyGroup>
    <DocumentationFile>Diamond.Core.Workflow.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
    <None Remove="Diamond.Patterns.WorkFlow.xml" />
    <None Remove="Services\Diamond.Core.Workflow.json" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Services\Diamond.Core.Workflow.json">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Humanizer.Core" Version="2.14.1" />
    <PackageReference Include="Microsoft.Extensions.Logging.Abstractions" Version="8.0.0" />
    <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.0" />
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="8.0.0">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="TryDisposable" Version="1.5.1" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\..\Shared\Diamond.Core.Abstractions\Diamond.Core.Abstractions.csproj" />
    <ProjectReference Include="..\..\System\Diamond.Core.System.TemporaryFolder\Diamond.Core.System.TemporaryFolder.csproj" />
    <ProjectReference Include="..\Diamond.Core.Workflow.State\Diamond.Core.Workflow.State.csproj" />
    <ProjectReference Include="..\Diamond.Core.Workflow.Abstractions\Diamond.Core.Workflow.Abstractions.csproj" />
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\..\Images\Diamond.Core.128x128.png">
      <Pack>True</Pack>
      <PackagePath>
      </PackagePath>
    </None>
  </ItemGroup>
</Project>