<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;net5.0</TargetFrameworks>
		<Product>Diamond.Core Patterns Library</Product>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<Version>5.0.6</Version>
		<FileVersion>5.0.6</FileVersion>
		<AssemblyVersion>5.0.6</AssemblyVersion>
		<Authors>Daniel M Porrey</Authors>
		<Description>Provides abstractions for Do pattern execution for ASP.NET Core MVC core projects.</Description>
		<PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/porrey/Diamond.Core-Patterns</PackageProjectUrl>
		<PackageIcon>Diamond.Core.128x128.png</PackageIcon>
		<RepositoryUrl>https://github.com/porrey/Diamond.Core-Patterns</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<NeutralLanguage>en-US</NeutralLanguage>
		<PackageIconUrl></PackageIconUrl>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
	</PropertyGroup>

	<PropertyGroup>
	  <DocumentationFile>Diamond.Core.AspNetCore.DoAction.Abstractions.xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Diamond.Core.AspNetCore.DoAction.Abstractions.xml" />
	</ItemGroup>

	<ItemGroup>
		<None Include="..\..\..\Images\Diamond.Core.128x128.png">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>

	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.AspNetCore.Mvc" Version="2.2.0" />
	  <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0">
	    <PrivateAssets>all</PrivateAssets>
	    <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
	  </PackageReference>
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\..\Shared\Diamond.Core.Abstractions\Diamond.Core.Abstractions.csproj" />
	</ItemGroup>

</Project>
