<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFramework>net9.0</TargetFramework>
		<Product>Diamond.Core Patterns Library</Product>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<Version>9.1.0</Version>
		<AssemblyVersion>9.1.0</AssemblyVersion>
		<FileVersion>9.1.0</FileVersion>
		<PackageProjectUrl>https://github.com/porrey/Diamond.Core-Patterns</PackageProjectUrl>
		<PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
		<RepositoryUrl>https://github.com/porrey/Diamond.Core-Patterns</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<Description>Provides the extensions for Swagger documentation in ASP.NET Core applications.</Description>
		<Copyright>Copyright Â© Daniel Porrey 2019-2025</Copyright>
		<Authors>Daniel M Porrey</Authors>
		<Company>Daniel M Porrey</Company>
		<PackageIcon>Diamond.Core.128x128.png</PackageIcon>
		<NeutralLanguage>en-US</NeutralLanguage>
		<PackageIconUrl>
		</PackageIconUrl>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
	</PropertyGroup>
	<PropertyGroup>
		<DocumentationFile>Diamond.Core.AspNetCore.Swagger.xml</DocumentationFile>
	</PropertyGroup>
	<ItemGroup>
		<None Remove="Diamond.Core.AspNetCore.Swagger.xml" />
	</ItemGroup>
	<ItemGroup>
		<PackageReference Include="Microsoft.AspNetCore.JsonPatch" Version="9.0.7" />
		<PackageReference Include="Swashbuckle.AspNetCore.Filters" Version="9.0.0" />
		<PackageReference Include="Swashbuckle.AspNetCore.SwaggerGen" Version="9.0.3" />
	</ItemGroup>
	<ItemGroup>
		<None Include="..\..\..\Images\Diamond.Core.128x128.png">
			<Pack>True</Pack>
			<PackagePath>
			</PackagePath>
		</None>
	</ItemGroup>
</Project>