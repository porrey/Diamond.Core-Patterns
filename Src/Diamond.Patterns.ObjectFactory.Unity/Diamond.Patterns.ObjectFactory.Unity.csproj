<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>netstandard2.0</TargetFramework>
		<PackageProjectUrl>https://github.com/porrey/Diamond.Patterns</PackageProjectUrl>
		<RepositoryUrl>https://github.com/porrey/Diamond.Patterns</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<Version>2.1.1</Version>
		<FileVersion>2.1.1</FileVersion>
		<AssemblyVersion>2.1.1</AssemblyVersion>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<Company>Daniel M Porrey</Company>
		<Authors>Daniel M Porrey</Authors>
		<PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
		<Description>This library provides a basic implementation of ObjectFactory for Unity.</Description>
		<AssemblyName>Diamond.Patterns.ObjectFactory.Unity</AssemblyName>
		<RootNamespace>Diamond.Patterns.ObjectFactory.Unity</RootNamespace>
		<PackageId>Diamond.Patterns.ObjectFactory.Unity</PackageId>
		<Product>Diamond.Patterns.ObjectFactory.Unity</Product>
		<PackageIconUrl>https://raw.githubusercontent.com/porrey/Diamond.Patterns/master/Images/Diamond.Patterns.png</PackageIconUrl>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="Unity" Version="5.11.1" />
		<PackageReference Include="Unity.Abstractions" Version="5.11.1" />
		<PackageReference Include="Unity.Configuration" Version="5.11.1" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Diamond.Patterns.Abstractions\Diamond.Patterns.Abstractions.csproj" />
	</ItemGroup>

</Project>
