<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>netstandard2.1;net5.0</TargetFrameworks>
		<Version>5.0.8</Version>
		<FileVersion>5.0.8</FileVersion>
		<AssemblyVersion>5.0.8</AssemblyVersion>
		<Authors>Daniel M. Porrey, Harshit Gindra</Authors>
		<Product>Diamond Pattern Library - Rules Pattern</Product>
		<Description>This library provides abstractions for the Rules pattern.</Description>
		<Copyright>Copyright Â© Daniel Porrey 2017-2020</Copyright>
		<PackageLicenseExpression>LGPL-3.0-or-later</PackageLicenseExpression>
		<PackageProjectUrl>https://github.com/porrey/Diamond.Core-Patterns</PackageProjectUrl>
		<RepositoryUrl>https://github.com/porrey/Diamond.Core-Patterns</RepositoryUrl>
		<RepositoryType>git</RepositoryType>
		<PackageIconUrl></PackageIconUrl>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageIcon>Diamond.Core.128x128.png</PackageIcon>
		<NeutralLanguage>English (United States)</NeutralLanguage>
		<Company>Daniel M. Porrey</Company>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<PublishRepositoryUrl>true</PublishRepositoryUrl>
		<EmbedUntrackedSources>true</EmbedUntrackedSources>
		<IncludeSymbols>true</IncludeSymbols>
		<SymbolPackageFormat>snupkg</SymbolPackageFormat>
		<ContinuousIntegrationBuild>true</ContinuousIntegrationBuild>
	</PropertyGroup>

	<PropertyGroup>
	  <DocumentationFile>Diamond.Core.Rules.Abstractions.xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
	  <None Remove="Diamond.Core.Rules.Abstractions.xml" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="..\..\..\Images\Diamond.Core.128x128.png">
	    <Pack>True</Pack>
	    <PackagePath></PackagePath>
	  </None>
	</ItemGroup>

	<ItemGroup>
	  <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0">
	    <PrivateAssets>all</PrivateAssets>
	    <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
	  </PackageReference>
	</ItemGroup>

</Project>
